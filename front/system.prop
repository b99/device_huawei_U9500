# LCD DENSITY
ro.sf.lcd_density=320

# RIL
audioril.lib=libhuawei-audio-ril.so
modem.audio=1
rild.libpath=/system/lib/libxgold-ril.so
rild.libargs=-d /dev/xmd-tty0
mobiledata.interfaces=pdp0,wlan0,gprs,ppp0,rmnet0
ro.config.hw_flashless=true
#
# Default network type.
# 0 => WCDMA preferred.
# 2 => WCDMA only
ro.telephony.default_network=2
#
# hsdpa+
ro.config.hspap_hsdpa_open=1
ro.ril.hsxpa=3
ro.ril.gprsclass=34
ro.ril.hsdpa.category=16
ro.ril.hsupa.category=6
ro.ril.enable.dtm=1

ro.config.hw_addsettingsdbex=1
ro.config.fm_type=libbcmfm_if

# For HSDPA low throughput
ro.ril.disable.power.collapse=1

# Ril sends only one RIL_UNSOL_CALL_RING, so set call_ring.multiple to false
ro.telephony.call_ring.multiple=0

# MASS STORAGE
ro.vold.umsdirtyratio=50
ro.config.internal_sdcard=yes

# WiFi
wifi.interface=wlan0
wifi.supplicant_scan_interval=15
ro.wifi.country=GB

# hardware info
ro.device.chipset=TI OMAP 4460
ro.device.cpu=1.2GHz dual-core Cortex-A9
ro.device.gpu=PowerVR SGX SGX540
ro.device.rear_cam=8 MP
ro.device.front_cam=1.3 MP
ro.device.screen_res=720x1280

# Enable OpenGL ES 2.0
ro.opengles.version=131072

# GPS
ro.config.hw_gps=47511
ro.allow.mock.location=1
# For the default value of agps
ro.ril.def.agps.mode=2

# Audio
ro.config.hw_dolby=true
ro.config.hw_dolby_config=front
dolby.audio.hdmi.channels=0
ro.config.hw_voicerecord=true
media.aac_51_output_enabled=true
ro.config.switchPrimaryVolume=true

media.stagefright.enable-player=true
media.stagefright.enable-meta=true
media.stagefright.enable-scan=false
media.stagefright.enable-http=true


# Huawei
com.ti.omap_enhancement=true
com.ti.omap_enhancement_s3d=true
ro.config.hw_GSensorOptimize=true
ro.config.hw_power_saving=true
ro.kernel.android.checkjni = 0
ro.config.hw_battery_percent=true
ro.config.hw_uninstall_dataapp=true
ro.config.fast_dormancy = false
keyguard.no_require_sim=true
ro.config.endstksession=true
ro.config.hw_temperature_warn=true
ro.config.callwait_vibrator=true
ro.config.callrecord.enabled=1
ro.config.hw_support_ipcall=true
ro.config.hw_ecclist_withcard=1+110,6+119,8+118
ro.config.hw_ecclist_nocard=1+110,6+119,8+118
persist.sys.actualpoweron=true
ro.config.hw_proximitySensor=true
ro.config.hw_globalEcc = true
ro.config.helix_enable=true
gsm.sim.num.pin2=3
gsm.sim.num.puk2=10
ro.config.hw_gcf_mms = true
ro.hwui.disable_scissor_opt=true
windowsmgr.max_events_per_sec=150
ro.max.fling_velocity=12000
ro.min.fling_velocity=8000
profiler.force_disable_err_rpt=1
profiler.force_disable_ulog=1
